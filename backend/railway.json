{
  "build": {
    "builder": "NIXPACKS",
    "nixpacksConfig": {
      "phases": {
        "setup": {
          "nixPkgs": ["python311", "pip", "postgresql"]
        },
        "install": {
          "cmds": ["pip install --upgrade pip", "pip install -r requirements.txt"]
        },
        "start": {
          "cmds": ["gunicorn contact.wsgi:application"]
        }
      }
    }
  }
}
